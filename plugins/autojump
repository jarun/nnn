#!/usr/bin/env sh

# Description: Autojump to directory using autojump
#
# Requires: Autojump   https://github.com/wting/autojump
#
# Shell: POSIX compliant
# Author: Marty Buchaus

if which autojump >/dev/null 2>&1; then
    printf "Autojump to: "
    read -r dir
    odir=$(autojump $dir)
    printf "%s" "0$odir" > "$NNN_PIPE"
else
    exit 1
fi
